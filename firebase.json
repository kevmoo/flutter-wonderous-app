{
  "hosting": {
    "site": "wonderous-kevmoo",
    "predeploy": "flutter build web --wasm",
    "public": "build/web/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "cross-origin-opener-policy",
            "value": "same-origin"
          },
          {
            "key": "cross-origin-embedder-policy",
            "value": "require-corp"
          }
        ]
      }, {
        "source": "/?(\\?*)",
        "headers": [
          {
            "key": "Link",
            "value": "</assets/FontManifest.json>; rel=preload; as=fetch; crossorigin,<assets/AssetManifest.bin.json>; rel=preload; as=fetch; crossorigin"
          }
        ]
      }
    ]
  }
}